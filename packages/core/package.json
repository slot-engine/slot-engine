{
  "name": "@slot-engine/core",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/slot-engine/slot-engine"
  },
  "homepage": "https://slot-engine.dev/docs/core",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "prebuild": "pnpm typecheck && pnpm copy-files",
    "build": "tsup ./index.ts --dts --format cjs,esm --sourcemap",
    "copy-files": "tsx ../../scripts/copy-core-files.ts"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "typescript": "5.9.3",
    "tsup": "8.5.0"
  },
  "dependencies": {
    "esbuild": "0.25.9"
  }
}