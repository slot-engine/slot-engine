{
  "name": "@slot-engine/panel",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/slot-engine/slot-engine"
  },
  "homepage": "https://slot-engine.dev/docs/panel",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build-server": "tsup ./server/index.ts --dts --format cjs,esm --sourcemap",
    "build-client": "cd ./client && tsc -b && vite build",
    "build": "pnpm run build-server && pnpm run build-client",
    "dev-server": "tsx watch ./dev.ts",
    "dev-client": "cd ./client && vite",
    "dev": "concurrently \"pnpm run dev-server\" \"pnpm run dev-client\""
  },
  "files": [
    "dist",
    "dist-client"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@ark-ui/react": "5.30.0",
    "@hono/node-server": "1.19.7",
    "@tabler/icons-react": "3.36.0",
    "@tanstack/react-query": "5.90.12",
    "@tanstack/react-router": "1.141.8",
    "@tanstack/router-plugin": "1.141.8",
    "chalk": "5.6.2",
    "cva": "1.0.0-beta.4",
    "hono": "4.11.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "3.4.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@fontsource-variable/archivo": "5.2.8",
    "@slot-engine/core": "0.1.10",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "9.2.1",
    "tailwindcss": "4.1.18",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  }
}